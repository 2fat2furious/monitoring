<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.controllers.TypeOfDevelopmentProgramDialogController"
            prefHeight="400.0" prefWidth="600.0">


    <children>
        <GridPane layoutX="24.0" layoutY="63.0" prefHeight="220.0" prefWidth="416.0">
            <children>
                <Label prefWidth="150.0" text="Название типа программы развития" GridPane.columnIndex="0" GridPane.rowIndex="0" />

                <TextField fx:id="name_pField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="164.0" minWidth="10.0" prefWidth="114.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="44.0" minHeight="0.0" prefHeight="39.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
        <Button layoutX="215.0" layoutY="323.0" mnemonicParsing="false" onAction="#handleOk" text="Сохранить" />
        <Button layoutX="329.0" layoutY="323.0" mnemonicParsing="false" onAction="#handleCancel" prefWidth="99.0" text="Назад" />
    </children>

</AnchorPane>
