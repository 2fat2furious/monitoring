<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.controllers.AcademicYearDialogController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <GridPane layoutX="24.0" layoutY="63.0" prefHeight="220.0" prefWidth="416.0">
            <children>
                <Label prefWidth="150.0" text="Год начала:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                <Label prefWidth="150.0" text="Год окончания:" GridPane.columnIndex="0" GridPane.rowIndex="1" />

                <TextField fx:id="yearInitialField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                <TextField fx:id="yearEndField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="164.0" minWidth="10.0" prefWidth="114.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="240.0" minWidth="10.0" prefWidth="219.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="44.0" minHeight="0.0" prefHeight="39.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="46.0" minHeight="0.0" prefHeight="46.0" vgrow="SOMETIMES" />
            </rowConstraints>
        </GridPane>
        <Label layoutX="63.0" layoutY="20.0" text="Добавление/Изменение данных об учебном годе" />
        <Button layoutX="215.0" layoutY="323.0" mnemonicParsing="false" onAction="#handleOk" text="Сохранить" />
        <Button layoutX="329.0" layoutY="323.0" mnemonicParsing="false" onAction="#handleCancel" prefWidth="99.0" text="Назад" />
    </children>
</AnchorPane>
